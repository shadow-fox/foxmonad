#!/bin/bash
if pgrep tint2; then
	pkill tint2
	exit 1
else
 	exec tint2 -c /storage/sfox/SystemConfig/dkenv/tint2/panellauncher.tint2rc &
	exec tint2 -c /storage/sfox/SystemConfig/dkenv/tint2/clock.tint2rc &
	exec tint2 -c /storage/sfox/SystemConfig/dkenv/tint2/customscripts.tint2rc &
	exec tint2 -c /storage/sfox/SystemConfig/dkenv/tint2/icons.tint2rc &
    exec tint2 -c /storage/sfox/SystemConfig/dkenv/tint2/workspaces.tint2rc &
fi
exit
